{
  "version": 2,
  "builds": [
    {
      "src": "/api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "/dist/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "/dist/favicon.ico",
      "use": "@vercel/static"
    },
    {
      "src": "/dist/css/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "/dist/js/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
